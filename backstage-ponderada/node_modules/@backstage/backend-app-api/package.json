    "@types/compression": "^1.7.0",
    "@types/fs-extra": "^11.0.0",
    "@types/http-errors": "^2.0.0",
    "@types/minimist": "^1.2.0",
    "@types/morgan": "^1.9.0",
    "@types/node-forge": "^1.3.0",
